<div class="row">
  <div class="col s3"></div>
  <div class="col s6">
    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="input-field col s12">
            <input placeholder="Enter game title" id="gameTitle" type="text" class="validate">
            <label for="gameTitle">Game Title</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <label>Select Game Owner</label>
            <select>
              <option value="???" selected>Choose your option</option>
              <option value="1">Option 1</option>
              <option value="2">Option 2</option>
              <option value="3">Option 3</option>
            </select>

          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <input placeholder="Enter minimum players" id="minPlayers" type="text" class="validate">
            <label for="minPlayers">Minimum Players</label>
          </div>
          <div class="input-field col s6">
            <input placeholder="Enter maximum players" id="maxPlayers" type="text" class="validate">
            <label for="maxPlayers">Maximum Players</label>
          </div>
        </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="password" type="password" class="validate">
        <label for="password">Password</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <input id="email" type="email" class="validate">
        <label for="email">Email</label>
      </div>
    </div>
    </form>
  </div>
</div>
<div class="col s3"></div>
</div>


