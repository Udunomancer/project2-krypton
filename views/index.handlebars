{{> header-block}}
<main>
    <div class="container">
        <div class="section">
            <img class="responsive-img" src="https://via.placeholder.com/720x250.png">
        </div>
        <div class="section">
            <form class="search-games">
                <div class="card">
                    <div class="card-content">
                        <input type="text" id="search-term" placeholder="Board Game Title"></text>
